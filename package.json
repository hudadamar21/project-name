{
  "name": "codestuff",
  "version": "1.0.0",
  "description": "Code Stuff adalah Website yang menyediakan semua yang dibutuhkan developers dari Tutorial, Article, Code Snippet, dan lain-lain",
  "scripts": {
    "dev": "npx ./node_modules/tailwindcss/lib/cli.js -i ./static/css/tailwind.css -o ./static/css/styles.css --jit",
    "watch": "npx tailwindcss -o ./static/css/styles.css --jit --watch",
    "commit": "git add . && git commit -m",
    "build": "npx ./node_modules/tailwindcss/lib/cli.js -i ./static/css/tailwind.css -o ./static/css/styles.css --jit --minify"
  },
  "keywords": [
    "blog"
  ],
  "author": "Huda Damar",
  "license": "ISC",
  "dependencies": {
    "tailwindcss": "^2.2.4"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@tailwindcss/line-clamp": "^0.2.1"
  }
}
